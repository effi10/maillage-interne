{
  "name": "maillage-interne-intelligente",
  "version": "1.0.0",
  "description": "Plugin WordPress Gutenberg pour automatiser le maillage interne avec IA",
  "main": "assets/js/blocks.js",
  "scripts": {
    "build": "wp-scripts build assets/js/blocks.js --output-path=assets/js/",
    "start": "wp-scripts start assets/js/blocks.js --output-path=assets/js/",
    "lint:js": "wp-scripts lint-js assets/js/",
